{
  "name": "mha",
  "version": "1.0.0",
  "description": "Message Header Analyzer",
  "engines": {
    "node": ">=12.13.0",
    "npm": ">=6.0.0"
  },
  "private": true,
  "scripts": {
    "watch": "webpack --watch --mode development",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "clean": "node tasks/clean.js",
    "start": "office-addin-debugging start ManifestDebugLocal.xml",
    "start:server": "office-addin-debugging start manifestDebugServer.xml",
    "stop": "office-addin-debugging stop ManifestDebugLocal.xml && office-addin-debugging stop manifestDebugServer.xml",
    "validate": "office-addin-manifest validate Manifest.xml",
    "test": "grunt test"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/stephenegriffin/mha.git"
  },
  "author": "Stephen Griffin",
  "license": "MIT",
  "config": {
    "app_to_debug": "outlook",
    "app_type_to_debug": "desktop",
    "dev_server_port": 44336
  },
  "bugs": {
    "url": "https://github.com/stephenegriffin/mha/issues"
  },
  "homepage": "https://github.com/stephenegriffin/mha#readme",
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "@types/office-js": "^1.0.325",
    "@types/qunit": "^2.19.5",
    "css-loader": "^6.7.1",
    "exports-loader": "^4.0.0",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^7.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "grunt": "^1.6.1",
    "grunt-contrib-connect": "^3.0.0",
    "grunt-contrib-qunit": "^6.2.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.6",
    "office-addin-debugging": "^5.0.5",
    "office-addin-dev-certs": "^1.7.10",
    "office-addin-dev-settings": "^2.0.3",
    "office-addin-manifest": "^1.8.1",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.15.0"
  },
  "dependencies": {
    "@microsoft/applicationinsights-web": "^3.0.1",
    "@microsoft/office-js": "^1.1.86",
    "codepage": "^1.15.0",
    "dayjs": "^1.11.7",
    "framework7": "^1.7.1",
    "framework7-icons": "^5.0.5",
    "jquery": "^3.6.0",
    "jwt-decode": "^3.1.2",
    "office-ui-fabric-js": "^1.5.0",
    "promise-polyfill": "8.3.0",
    "qunit": "^2.19.4",
    "stacktrace-js": "^2.0.2"
  },
  "-vs-binding": {
    "BeforeBuild": [
      "build:dev"
    ],
    "Clean": [
      "clean"
    ]
  }
}
